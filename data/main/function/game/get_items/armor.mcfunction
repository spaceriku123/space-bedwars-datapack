execute if score @s rank_armor matches 1 run item replace entity @s armor.legs with chainmail_leggings[unbreakable={show_in_tooltip:false}]
execute if score @s rank_armor matches 1 run item replace entity @s armor.feet with chainmail_boots[unbreakable={show_in_tooltip:false}]

execute if score @s rank_armor matches 2 run item replace entity @s armor.legs with iron_leggings[unbreakable={show_in_tooltip:false}]
execute if score @s rank_armor matches 2 run item replace entity @s armor.feet with iron_boots[unbreakable={show_in_tooltip:false}]

execute if score @s rank_armor matches 3 run item replace entity @s armor.legs with diamond_leggings[unbreakable={show_in_tooltip:false}]
execute if score @s rank_armor matches 3 run item replace entity @s armor.feet with diamond_boots[unbreakable={show_in_tooltip:false}]
